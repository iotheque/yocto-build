[project]
org='mapio'
name='bsp'

[build]
command='kas build project.yml'
flavors='bundle bundle-with-kernel bundle-with-uboot-kernel'

[bundle]
command="kas shell project.yml -c 'bitbake mapio-bundle-image'"

[bundle-with-kernel]
command="kas shell project.yml -c 'bitbake mapio-bundle-image-with-kernel'"

[bundle-with-uboot-kernel]
command="kas shell project.yml -c 'bitbake mapio-bundle-image-with-uboot-kernel'"
